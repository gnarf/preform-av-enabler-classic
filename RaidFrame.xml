						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnMouseUp>
						self:GetParent().isResizing = nil;
					</OnMouseUp>
					<OnMouseDown>
						if ( PreformAVEnabler.IfSecure() ) then
							return
						end

						local _;
						_, self:GetParent().startPos = GetCursorPosition();
						self:GetParent().startPos = math.floor(self:GetParent().startPos);
						self:GetParent().startHeight = self:GetParent():GetHeight();
						self:GetParent().isResizing = true;
					</OnMouseDown>
					<OnEnter>
						PreformAVEnablerResizeButton_OnEnter();
					</OnEnter>
					<OnLeave>
						ResetCursor();
					</OnLeave>
				</Scripts>
			</Frame>

			<Frame name="PreformAVEnablerDropDown" inherits="UIDropDownMenuTemplate" id="1" hidden="true">
				<Size>
					<AbsDimension x="10" y="10"/>
				</Size>
				<Anchors>
					<Anchor point="TOPLEFT">
						<Offset>
							<AbsDimension x="10" y="-10"/>
						</Offset>
					</Anchor>
				</Anchors>
				<Scripts>
					<OnLoad>
						PreformAVEnablerDropDown_OnLoad(self);
					</OnLoad>
				</Scripts>
			</Frame>

		</Frames>

		<Scripts>
			<OnLoad>
				PreformAVEnabler.Frame_OnLoad(self);
			</OnLoad>
			<OnEvent>
				PreformAVEnablerFrame_OnEvent(event, ...);
			</OnEvent>
			<OnShow>
				if ( PreformAVEnabler.neverUpdated ~= 2 ) then
					PreformAVEnabler.RequestStatus();
					PreformAVEnabler.neverUpdated = 2;
				end

				PreformAVEnabler.Update();
			</OnShow>
			<OnHide>
				PlaySound("igMainMenuQuit");
			</OnHide>
			<OnUpdate>
				PreformAVEnablerFrame_OnUpdate(self, elapsed);
			</OnUpdate>
			<OnMouseUp>
				if ( self.isMoving ) then
					self:StopMovingOrSizing();
					self.isMoving = false;
				end
			</OnMouseUp>
			<OnMouseDown>
				if (not self.IsLocked and button == "LeftButton") then
					self:StartMoving();
					self.isMoving = true;
				end
			</OnMouseDown>
		</Scripts>
	</Frame>
	
