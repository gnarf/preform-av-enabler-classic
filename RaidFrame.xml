	<!-- Automate template -->
	<CheckButton name="PreformAVEnablerAutomateCheckButtonTemplate" inherits="UICheckButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="24" y="24"/>
		</Size>
		<Scripts>
			<OnShow>
				if ( PreformAVEnabler.automateBG == self:GetID() ) then
					self:SetChecked(1);
				else
					self:SetChecked(0);
				end
				--_G[self:GetName().."Text"]:SetText(PREFORM_AV_ENABLER_AUTOMATE);
			</OnShow>
			<OnClick>
				PreformAVEnablerAutomateCheckButton_OnClick(self);
			</OnClick>
			<OnEnter>
				GameTooltip:SetOwner(self, "ANCHOR_LEFT");
				GameTooltip:SetText(PREFORM_AV_ENABLER_TOOLTIP5, nil, nil, nil, nil, 1);
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
		</Scripts>
	</CheckButton>

