	<!-- Queue buttons template -->
	<Button name="PreformAVEnablerFrameQueueButtonTemplate" inherits="GameMenuButtonTemplate" text="PREFORM_AV_ENABLER_QUEUE" virtual="true">
		<Size>
			<AbsDimension x="70" y="20"/>
		</Size>
		<Scripts>
			<OnClick>
				PreformAVEnabler.RaidQueue(self:GetID());
			</OnClick>
			<OnEnter>
				GameTooltip:SetOwner(self, "ANCHOR_LEFT");
				GameTooltip:SetText(PREFORM_AV_ENABLER_TOOLTIP1, nil, nil, nil, nil, 1);
			</OnEnter>
			<OnLeave>
				GameTooltip:Hide();
			</OnLeave>
			<OnLoad>
				self:SetPoint("TOPLEFT", "PreformAVEnablerMajorityText"..self:GetID(), "BOTTOMLEFT", 0, -5);
			</OnLoad>
		</Scripts>
	</Button>

